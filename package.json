{
  "name": "es6-module-boilerplate",
  "version": "1.0.0",
  "description": "simple es6 boilerplate for developing npm modules",
  "main": "library.bundle.js",
  "scripts": {
    "dev": "node webpack.dev-server.js",
    "build": "webpack --config=webpack.config.js",
    "clean": "rm library.bundle.js && rm examples/bundle.js",
    "test": "mocha --compilers js:babel-core/register ./test/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/connorwalsh/es6-module-boilerplate.git"
  },
  "keywords": [
    "es6",
    "npm",
    "boilerplate"
  ],
  "author": "c",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/connorwalsh/es6-module-boilerplate/issues"
  },
  "homepage": "https://github.com/connorwalsh/es6-module-boilerplate#readme",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.2",
    "webpack-dev-server": "^1.14.1"
  }
}
